your-domain.com {
    root * /var/www/website-metatah/public
    encode gzip
    file_server

    php_fastcgi 127.0.0.1:9011

    @static {
        file
        path *.ico *.css *.js *.gif *.jpg *.jpeg *.png *.svg *.woff *.woff2 *.ttf *.eot
    }
    header @static Cache-Control max-age=31536000

    log {
        output file /var/log/caddy/website-metatah-access.log
    }
}
